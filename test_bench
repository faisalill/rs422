    %store/vec4 v0x584cc5f8c0c0_0, 0, 1;
T_6.3 ;
    %load/vec4 v0x584cc5f8bc90_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x584cc5f8c000_0, 0, 1;
    %load/vec4 v0x584cc5f8bc90_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %store/vec4 v0x584cc5f8bc90_0, 0, 8;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x584cc5f8baf0_0;
    %pushi/vec4 1, 0, 4;
    %add;
    %store/vec4 v0x584cc5f8baf0_0, 0, 4;
T_6.0 ;
