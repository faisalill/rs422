T_6 ;
    %wait E_0x584cc5f730e0;
    %load/vec4 v0x584cc5f8baf0_0;
    %pad/u 32;
    %cmpi/u 8, 0, 32;
    %jmp/0xz  T_6.0, 5;
    %pushi/vec4 0, 0, 1;
