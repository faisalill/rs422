v0x584cc5f8c000_0 .var "op_bit", 0 0;
v0x584cc5f8c0c0_0 .var "start", 0 0;
v0x584cc5f8c180_0 .var "stop", 0 0;
E_0x584cc5f730e0 .event posedge, v0x584cc5f5f670_0;
    .scope S_0x584cc5f5f3f0;
T_5 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x584cc5f8be80_0, 0, 1;
