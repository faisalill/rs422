    %end;
S_0x584cc5f5f3f0 .scope module, "rs232_t" "rs232_transmitter" 2 44, 3 1 0, S_0x584cc5f32170;
 .timescale -9 -9;
    .port_info 0 /INPUT 8 "data";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "idle";
    .port_info 3 /OUTPUT 1 "start";
    .port_info 4 /OUTPUT 1 "stop";
    .port_info 5 /OUTPUT 1 "op_bit";
v0x584cc5f5f670_0 .net "clk", 0 0, v0x584cc5f8c300_0;  1 drivers
v0x584cc5f8baf0_0 .var "counter", 3 0;
v0x584cc5f8bbd0_0 .net "data", 7 0, L_0x752ffc2b8018;  alias, 1 drivers
v0x584cc5f8bc90_0 .var "data_copy", 7 0;
v0x584cc5f8bd70_0 .var "first_bit", 0 0;
v0x584cc5f8be80_0 .var "idle", 0 0;
